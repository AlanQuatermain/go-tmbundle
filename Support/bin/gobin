#!/usr/bin/env bash

. goarch

if test -n "$GOBIN"; then
	GOPREFIX=${GOBIN}/
fi

O=`go_arch_identifier $GOARCH`
AS=${GOPREFIX}${O}a
CC=${GOPREFIX}${O}c
GC=${GOPREFIX}${O}g
LD=${GOPREFIX}${O}l
NM=${GOPREFIX}${O}nm
AR=${GOPREFIX}gopack
MK=${GOPREFIX}gomake
GT=${GOPREFIX}gotest

require_cmd "$GC"

if test -z "$GOBIN"; then
	GOBIN="$(dirname "$(/usr/bin/which "$GC")")"
fi

export GOBIN
